class Stack {
    def init {
        @data = []
    }

    def push(x) {
        @data.push(x)
    }

    def pop {
        @data.pop
    }

    def top {
        @data[@data.length - 1]
    }
}

s = Stack.new
s.push(1)
s.push(2)
s.push(3)
s.push(4)
s.push(10)
println(s.pop) #=> 10
println(s.top) #=> 4
println(s.top) #=> 4
